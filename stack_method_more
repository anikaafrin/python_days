class stack(object):
    def __init__(self):
        self.items = [] 
    def push(self, item):
        #push the element 
        self.items.append(item) 
        #print(self.items, end=" ")
    def pop(self):
        self.items.pop() 
        pass 
    def peek(self):
        if self.items :
            return self.items[-1] 
        else:
            return None 
    def size(self):
        if self.items:
            return len(self.items) 
        else: 
            return None 
    def isempty(self):
        if self.items == []:
            return True
        else: 
            return False 
if __name__ == "__main__":
    stack = stack() 
    stack.push(1)
    stack.push(2)
    stack.push(3)
    stack.push(4)
    stack.push(5)
    stack.push(8)
    print(stack.peek())
    stack.pop() 
    print(stack.size())
    
            
            
            
            
            
            
            
            
        
